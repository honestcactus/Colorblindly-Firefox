{
    "action": {
        "default_popup": "popup/popup.html",
        "default_title": "Colorblindly"
    },
    "description": "Simulates colorblindness in your web browser.",
    "homepage_url": "https://github.com/oftheheadland",
    "host_permissions": [
        "<all_urls>"
    ],
    "icons": {
        "128": "images/newlogo128.png",
        "16": "images/newlogo128.png",
        "48": "images/newlogo128.png"
    },
    "key": "",
    "manifest_version": 3,
    "name": "Colorblindly",
    "permissions": [
        "activeTab",
        "scripting",
        "tabs"
    ],
    "update_url": "https://github.com/oftheheadland/Colorblindly",
    "version": "2.5",
    "web_accessible_resources": [
        {
            "matches": [
                "<all_urls>"
            ],
            "resources": [
                "filters/achromatomaly.js",
                "filters/achromatopsia.js",
                "filters/deuteranomaly.js",
                "filters/deuteranopia.js",
                "filters/normal.js",
                "filters/protanomaly.js",
                "filters/protanopia.js",
                "filters/tritanomaly.js",
                "filters/tritanopia.js"
            ]
        }
    ],
    "browser_specific_settings": {
        "gecko": {
            "id": "colorblindly@firefox"
        }
    }
}